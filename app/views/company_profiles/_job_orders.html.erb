<h1>Listing job_orders</h1>
<% if @job_orders.any? %>
<table>
  <thead>
    <tr>
      <th>Job title</th>
      <th>Company profile</th>
      <th>Fill date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @job_orders.each do |job_order| %>
      <tr>
        <td><%= job_order.job_title %></td>
        <td><%= job_order.company_profile %></td>
        <td><%= job_order.fill_date %></td>
        <td><%= link_to 'Show', job_order if can? :read, job_order %></td>
        <td><%= link_to 'Edit', edit_job_order_path(job_order) if can? :update, job_order %></td>
        <td><%= link_to 'Destroy', job_order, method: :delete, data: { confirm: 'Are you sure?' } if can? :delete, job_order %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<% else %>
No Job Orders.
<% end %>