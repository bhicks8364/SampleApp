<h2 class="sub-header">All Agencies</h2>

<div class="table-responsive">
<table class="table table-striped">
  <thead>
    <tr>
      <th>Agency Name</th>
      <th>Company ID | Admin_User ID</th>
      <th>Company Admin Name | Company Admin Email</th>
      <th>Created</th>
    

      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>

  <% @agency_profiles.each do |agency| %>

      <tr>
        <td><%= agency.name %></td>
        <td><%= agency.id %>        |       <%= agency.user.id %></td>
        <td><%= agency.user.name %> | <%= agency.user.email %></td>
        <td><%= time_ago_in_words(agency.created_at) %></td>
        
        <td><%= link_to 'Show', agency %></td>
      </tr>
  <% end %>
 
  </tbody>
</table>
</div>
